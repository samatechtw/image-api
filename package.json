{
  "name": "@samatech/image-api",
  "version": "0.0.0",
  "description": "Service that allows uploading an image, validating it, and pushing to an external data service such as AWS S3.",
  "main": "src/main.ts",
  "scripts": {
    "prepare": "husky install",
    "dev:server": "nest start --watch",
    "dev:web": "webpack --config webpack.config.web.ts --watch --mode development",
    "example": "npm -C example run dev",
    "dev:example": "concurrently --kill-others \"npm run dev:web\" \"npm run example:dev\"",
    "build:image-rs": "cd src/processor/image-rs-processor && wasm-pack build --target web --out-dir pkg-web && wasm-pack build --target nodejs --out-dir pkg-node",
    "build:server": "nest build",
    "build:web": "webpack --config webpack.config.web.ts --mode production",
    "start": "nest start",
    "test:unit": "jest --runInBand --config jest.config.unit.ts",
    "test:e2e": "jest --runInBand --config jest.config.e2e.ts",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier -w \"{**/*,*}.{ts,js,json}\"",
    "format:check": "prettier --check \"{**/*,*}.{ts,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samatechtw/image-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samatechtw/image-api/issues"
  },
  "homepage": "https://github.com/samatechtw/image-api#readme",
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/testing": "^8.2.6",
    "@types/app-root-path": "^1.2.4",
    "@types/convict": "^6.1.1",
    "@types/estree": "^0.0.51",
    "@types/express": "^4.17.13",
    "@types/form-data": "^2.5.0",
    "@types/jest": "26.0.24",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.33",
    "@types/node-fetch": "^2.6.1",
    "@types/p-wait-for": "^3.0.1",
    "@types/sharp": "^0.29.0",
    "@types/supertest": "^2.0.11",
    "@types/webpack": "^5.28.0",
    "@types/workerpool": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "aws-sdk-client-mock": "^0.6.2",
    "babel-jest": "^27.4.6",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "27.0.3",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "rollup": "^2.63.0",
    "rollup-plugin-ignore": "^1.0.10",
    "semantic-release": "^17.4.7",
    "semantic-release-gitmoji": "^1.3.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "27.0.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
