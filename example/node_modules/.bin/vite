#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sam/git/samatech/image-api/node_modules/.pnpm/vite@2.9.4/node_modules/vite/bin/node_modules:/Users/sam/git/samatech/image-api/node_modules/.pnpm/vite@2.9.4/node_modules/vite/node_modules:/Users/sam/git/samatech/image-api/node_modules/.pnpm/vite@2.9.4/node_modules:/Users/sam/git/samatech/image-api/node_modules/.pnpm/node_modules:/Users/sam/git/samatech/image-api/node_modules:/Users/sam/git/samatech/node_modules:/Users/sam/git/node_modules:/Users/sam/node_modules:/Users/node_modules:/node_modules:/Users/sam/git/samatech/image-api/example/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sam/git/samatech/image-api/node_modules/.pnpm/vite@2.9.4/node_modules/vite/bin/node_modules:/Users/sam/git/samatech/image-api/node_modules/.pnpm/vite@2.9.4/node_modules/vite/node_modules:/Users/sam/git/samatech/image-api/node_modules/.pnpm/vite@2.9.4/node_modules:/Users/sam/git/samatech/image-api/node_modules/.pnpm/node_modules:/Users/sam/git/samatech/image-api/node_modules:/Users/sam/git/samatech/node_modules:/Users/sam/git/node_modules:/Users/sam/node_modules:/Users/node_modules:/node_modules:/Users/sam/git/samatech/image-api/example/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@2.9.4/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@2.9.4/node_modules/vite/bin/vite.js" "$@"
fi
