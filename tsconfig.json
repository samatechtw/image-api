{
  "compilerOptions": {
    "moduleResolution": "Node",
    "target": "ESNext",
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "references": [
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "include": ["src/**/*.ts", "src/**/*.js", "pathStore.ts"],
  "exclude": ["node_modules"]
}
