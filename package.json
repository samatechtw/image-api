{
  "name": "image-api",
  "version": "0.0.0",
  "description": "A common requirement in apps we develop is uploading an image from the client, validating on the server, and pushing to an external data service such as AWS S3.",
  "main": "src/main.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samatechtw/image-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samatechtw/image-api/issues"
  },
  "homepage": "https://github.com/samatechtw/image-api#readme",
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-wasm": "^5.1.2",
    "@types/estree": "0.0.50",
    "@types/express": "^4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@types/p-wait-for": "^3.0.1",
    "@types/sharp": "^0.29.0",
    "@types/supertest": "^2.0.11",
    "@types/workerpool": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "~4.28.5",
    "@typescript-eslint/parser": "~4.28.5",
    "babel-jest": "^27.4.6",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "27.0.3",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "rollup": "^2.63.0",
    "semantic-release": "^17.4.7",
    "semantic-release-gitmoji": "^1.3.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@squoosh/lib": "^0.4.0",
    "@types/bull": "^3.15.7",
    "@types/lodash": "^4.14.178",
    "@types/svgo": "^2.6.0",
    "@types/uuid": "^8.3.4",
    "bull": "^4.2.1",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "p-wait-for": "^4.1.0",
    "pngquant": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "sharp": "^0.29.1",
    "svgo": "^2.8.0",
    "uuid": "^8.3.2",
    "workerpool": "^6.1.5"
  }
}
