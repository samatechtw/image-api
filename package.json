    "@types/bull": "^3.15.7",
    "@types/lodash": "^4.14.178",
    "@types/svgo": "^2.6.0",
    "@types/uuid": "^8.3.4",
    "app-root-path": "^3.0.0",
    "bull": "^3.29.3",
